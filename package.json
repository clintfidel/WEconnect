{
  "name": "weconnect",
  "version": "1.0.0",
  "description": "WeConnect provides a platform that brings businesses and individuals together. This platform creates awareness for businesses and gives the users the ability to write reviews about the businesses they have interacted with.",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production babel-node server.js",
    "start-server": "NODE_ENV=development nodemon --watch  server --exec babel-node server.js",
    "start-server:dummy": "nodemon --watch dummyServer --exec babel-node server.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "server-dummy:test": "nyc --reporter=html --reporter=text mocha --exit --timeout 7000 --compilers js:babel-register dummyServer/test || true",
    "lint": "eslint ./ || true"
  },
  "nyc": {
    "exclude": [
      "dummyServer/test/**.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clintfidel/WEconnect.git"
  },
  "author": "clintfidel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/clintfidel/WEconnect/issues"
  },
  "homepage": "https://github.com/clintfidel/WEconnect#readme",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-eslint": "^8.2.2",
    "chai": "^4.1.2",
    "codeclimate-test-reporter": "^0.5.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "mocha": "^5.0.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.17.1"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "dotenv": "^5.0.1",
    "expect": "^22.4.0",
    "express": "^4.16.2",
    "express-validator": "^5.0.3",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.5",
    "nyc": "^11.4.1",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.36.0",
    "supertest": "^3.0.0",
    "winston": "^2.4.0"
  }
}
